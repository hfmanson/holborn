;
; HOLBORN INTERRUPT VECTOREN
; FOR FLOPPY AND HARD DISK
;
;	HB	1-6-1982
	ASEG
	ORG	3000H
	.Z80
;
;
	GLOBAL	FINTVC
	GLOBAL	F_CTCV
	GLOBAL	F_DMAV
	GLOBAL	HDCTVC
	GLOBAL	HDDMVC
;
	EXTRN	FICTC0
	EXTRN	FICTC1
	EXTRN	FICTC2
	EXTRN	FICTC3
;
	EXTRN	FIDMAF
;
	EXTRN	HDCT0I
	EXTRN	HDCT1I
	EXTRN	HDCT2I
	EXTRN	HDCT3I
;
	EXTRN	HDMAIP
;
;

NOPGEN	MACRO
	COND	LAB1 GT 0
LAB1	DEFL	(LAB1+1) AND 07H
	NOPGEN
	NOP
	ENDC
	ENDM
;
NOPG2	MACRO
	COND	(HIGH(LAB2+20)) GT (HIGH(LAB2))
LAB2	DEFL	LAB2+1
	NOPG2
	NOP
	ENDC
	ENDM
;
; INTERRUPT VECTOR
;
 
LAB1	DEFL	LOW($ AND 07H)
	NOPGEN			; GENEREER 8 VOUD
;
LAB2	DEFL	$
	NOPG2			; CHECK BYTE GRENS
 
FINTVC:
F_CTCV:	DEFW	FICTC0		; CTC0 INTERRUPT
	DEFW	FICTC1		; CTC1 INTERRUPT
	DEFW	FICTC2		; CTC2 (FDC) INTERRUPT
	DEFW	FICTC3		; CTC3 - TIME OUT - INTERRUPT
HDCTVC:
	DEFW	HDCT0I		; HARD DISK CTC INT VECTORE
	DEFW	HDCT1I
	DEFW	HDCT2I
	DEFW	HDCT3I		; TIME OUT VECTOR
;
;
; DMA INTERRUPT
;
F_DMAV:	DEFW	FIDMAF		; DMA / FDC INTERRUPT
HDDMVC:
	DEFW	HDMAIP		; HARD DISK DMA INT VECTOR
;

	END
;
